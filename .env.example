# Copy this file to .env and fill in your actual values

# Urbit Configuration
URBIT_SHIP_URL=http://localhost:8080
URBIT_SESSION_COOKIE=your-session-cookie-here

# AI Analysis API Key
LLAMA_API_KEY=your-llama-api-key-here

# Ship to send reports to (your ship name)
URBIT_RECIPIENT_SHIP=~your-ship

# GitHub Integration (Optional - for web dashboard)
GITHUB_TOKEN=your-github-token-here
GITHUB_REPO_OWNER=your-username
GITHUB_REPO_NAME=urbit-analytics

# Analysis Configuration (Optional)
ANALYSIS_INTERVAL_MINUTES=60
ACTIVITY_LOOKBACK_HOURS=24
MIN_MESSAGES_FOR_ANALYSIS=5